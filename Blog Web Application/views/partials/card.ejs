<li class="card">
    <div>            
        <div class="card-content">
            <h2> <%= content %> </h2>
        </div>
        <div class="card-foot">
            <h2> <%= postTime %> </h2>
            <form action="/delete" method="post">
                <button class="del-btn" name="i" type="submit" value=<%= i %>>
                    <img src="https://cdn.pixabay.com/photo/2017/11/10/05/24/delete-2935433_960_720.png" alt="Del">
                </button>
            </form>
        </div>
    </div>
</li>
